
proc fizzbuzz: nil(n: num) {
    # Executes the fizzbuzz game for n iterations

    as (i: num = 1; i <= n; i = i + 1) {

        if (i % 15 == 0) {
            say 'fizzbuzz';

        } elif (i % 3 == 0) {
            say 'fizz';

        } elif (i % 5 == 0) {
            say 'buzz';

        } else {
            say i;
        }
    }
}

fizzbuzz(100);
